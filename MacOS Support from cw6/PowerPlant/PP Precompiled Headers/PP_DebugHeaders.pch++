// ===========================================================================//	PP_DebugHeaders.pch++		PowerPlant 2.1		©1998-2000 Metrowerks Inc.// ===========================================================================////	Source for precompiled header for PowerPlant headers////	This file #includes most header files for the PowerPlant library,//	as well as most of the Toolbox headers used by the PowerPlant library//	with all debugging symbols defined.	// This same file works for PowerPC, CFM68K, and 68K. We check the target	// at compile time and specify the appropriate output file name.#if __POWERPC__	#pragma precompile_target "PP_DebugHeadersPPC++"#elif __CFM68K__	#pragma precompile_target "PP_DebugHeadersCFM68K++"#else	#pragma precompile_target "PP_DebugHeaders68K++"#endif#pragma once on#include "PP_DebugHeaders.cp"